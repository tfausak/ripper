{
  "devDependencies": {
    "eslint": "3.18.0",
    "mocha": "3.2.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:all",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "consistent-return": "off",
      "indent": [
        "error",
        2
      ],
      "line-comment-position": "off",
      "max-lines": "off",
      "max-statements": "off",
      "no-bitwise": "off",
      "no-console": "off",
      "no-constant-condition": "off",
      "no-inline-comments": "off",
      "no-magic-numbers": "off",
      "no-mixed-operators": "off",
      "one-var": [
        "error",
        "never"
      ],
      "padded-blocks": "off",
      "quote-props": [
        "error",
        "as-needed"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "name": "ripper",
  "private": true,
  "scripts": {
    "eslint": "eslint",
    "mocha": "mocha",
    "test": "eslint . && mocha"
  },
  "version": "0.0.0"
}
